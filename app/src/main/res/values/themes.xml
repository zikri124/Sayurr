<resources xmlns:tools="http://schemas.android.com/tools">
 <!-- Base application theme. -->
 <style name="Theme.Sayurr" parent="Theme.AppCompat.Light.NoActionBar">
  <!-- Primary brand color. -->
  <item name="colorPrimary">@color/Primary</item>
  <item name="colorPrimaryVariant">#FD5E53</item>
  <item name="colorOnPrimary">@color/white</item>
  <!-- Secondary brand color. -->
  <item name="colorSecondary">@color/teal_200</item>
  <item name="colorSecondaryVariant">#F5F5F5</item>
  <item name="colorOnSecondary">#989898</item>
  <!-- Status bar color. -->
  <item name="android:statusBarColor" tools:targetApi="l">@color/Primary</item>
  <!-- Customize your theme here. -->
 </style>

 <style name="Theme.Sayurr.NoActionBar">
  <item name="windowActionBar">false</item>
  <item name="windowNoTitle">true</item>
 </style>

 <style name="Theme.Sayurr.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

 <style name="Theme.Sayurr.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

 <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
  <item name="cornerSize">50%</item>
 </style>
</resources>